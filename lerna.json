{
  "packages": [
    "packages/chart-advisor",
    "packages/datawizard/*",
    "packages/knowledge"
  ],
  "npmClient": "yarn",
  "command": {
    "version": {
      "allowBranch": "master",
      "gitRemote": "upstream",
      "--no-push": true,
      "--no-commit-hooks": true
    },
    "publish": {
      "registry": "https://registry.npmjs.org/"
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-lockfile"
      ]
    }
  },
  "version": "0.1.3-alpha.22"
}
