{
  "name": "@antv/auto-chart",
  "version": "1.0.0",
  "description": "A React component for chart use",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx './'",
    "lint-fix": "npm run lint -- --fix",
    "prettier": " prettier --check '**/*' --ignore-path=../../.prettierignore",
    "prettier-fix": "npm run prettier -- --write",
    "clean": "rimraf lib esm dist",
    "format": "npm run prettier-fix && npm run lint-fix",
    "build-bak": "rollup -c",
    "test": "jest",
    "test-live": "DEBUG_MODE=1 jest --watch"
  },
  "author": {
    "name": "AntV",
    "url": "https://antv.vision/"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "eslint": "^7.32.0",
    "jest": "^24.9.0",
    "jest-electron": "^0.1.11",
    "jest-extended": "^0.11.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@antv/antv-spec": "^0.1.0-alpha.4",
    "@antv/chart-advisor": "^2.0.0-alpha.1",
    "@antv/g2plot-schemas": "^1.2.1",
    "@antv/knowledge": "^1.1.4",
    "@antv/thumbnails": "^1.3.0",
    "@antv/util": "^2.0.14",
    "antd": "^4.16.13",
    "lodash": "^4.17.21",
    "react-copy-to-clipboard": "^5.0.4",
    "schema-util": "^3.4.6"
  }
}
